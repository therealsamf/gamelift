{
  "name": "@kontest/gamelift",
  "version": "0.2.0",
  "description": "NodeJS bindings for AWS Gamelift server SDK",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc",
    "build:sourceMaps": "tsc --sourceMap --outDir bin",
    "prepare": "npm run build",
    "prettier": "prettier --write lib/",
    "lint": "eslint lib/*.ts",
    "docs": "typedoc --stripInternal",
    "docs:internal": "typedoc",
    "test": "mocha",
    "test:unit": "mocha -g \"unit\"",
    "coverage": "nyc mocha --require source-map-support/register",
    "coverage:report": "nyc report --reporter=lcov"
  },
  "keywords": [
    "nodejs",
    "aws",
    "gamelift",
    "kontest"
  ],
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "author": "therealsamf",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-gamelift-node": "0.1.0-preview.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.7",
    "@types/sinon": "^9.0.8",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "socket.io": "^3.0.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@kontest/gamelift-pb": "^0.1.8",
    "debug": "^4.2.0",
    "protobufjs": "^6.10.1",
    "socket.io-client": "^3.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
